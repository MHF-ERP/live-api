  "scripts": {
    "db:seed": "ts-node -r tsconfig-paths/register prisma/seeds/seed.ts",
    "seed": "ts-node auto2.seed.ts",
    "build": "npm run build:dist",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:dist": "nest build",
    "majestic": "majestic --port 2000",
    "test:ci": "npm run test -- --runInBand --detectOpenHandles --forceExit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js && npm run compare:swagger",
    "start:dev": "concurrently \"nest start --watch\"",
    "dev:full": "concurrently \"npm run test:watch\" \"npm run start:dev\" \"npm run watch:report\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ui": "majestic",
    "test:report": "jest --config ./test.config.ts --reporters=default --reporters=jest-html-reporter",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky && husky install",
    "db:sync": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "locale": "node translate.ts",
    "compare:swagger": "ts-node scripts/swagger-compare.ts"
  },
