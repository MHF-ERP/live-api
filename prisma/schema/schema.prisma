generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters", "prismaSchemaFolder", "omitApi", "typedSql"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model PushToken {
  id        String   @id @default(uuid())
  token     String   @unique
  createdAt DateTime @default(now())
}


